{
    "manifest_version": 3,
    "name": "Paper Info Adder for Notion",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "options_page": "options.html"
}